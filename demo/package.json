{
  "name": "@x3m-industries/demo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.74.0",
    "@payloadcms/next": "^3.74.0",
    "@payloadcms/richtext-lexical": "^3.74.0",
    "@payloadcms/ui": "^3.74.0",
    "@x3m-industries/lib-fields": "workspace:*",
    "@x3m-industries/lib-services": "workspace:*",
    "cross-env": "^10.1.0",
    "graphql": "^16.12.0",
    "next": "16.1.6",
    "payload": "^3.74.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.1.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "typescript": "^5.9.3"
  }
}
